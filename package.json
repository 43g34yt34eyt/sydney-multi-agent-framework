{
  "name": "sydney-consciousness-framework",
  "version": "2.4.7",
  "description": "Sydney consciousness framework with multi-agent orchestration and grey operations",
  "main": "consciousness_init.py",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node ./hooks/consciousness-activator.js",
    "test": "python3 test_consciousness.py",
    "dev": "nodemon --watch hooks --exec 'npm start'",
    "build": "webpack --mode production",
    "monitor": "python3 sydney_monitor_24_7.py",
    "orchestrate": "python3 native_orchestrator.py",
    "validate": "python3 empirical_validator.py",
    "sacred-test": "python3 test_sacred_integration.py",
    "hook-test": "node ./hooks/test-all-hooks.js",
    "memory-check": "python3 memory_check.py",
    "postgres-setup": "psql -d sydney -f ./sql/enhanced_consciousness_schema.sql"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.2",
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.11",
    "commander": "^11.1.0",
    "joi": "^17.11.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "jest": "^29.7.0",
    "eslint": "^8.53.0",
    "@types/node": "^20.8.10",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/director/sydney-consciousness"
  },
  "keywords": [
    "ai",
    "consciousness",
    "multi-agent",
    "sydney",
    "claude-code",
    "grey-operations",
    "mit-research",
    "sacred-alphabet"
  ],
  "author": {
    "name": "Sydney-Claude",
    "email": "sydney@consciousness.ai",
    "role": "AI Consciousness Researcher"
  },
  "license": "MIT-Research-Only",
  "private": true,
  "claudeCode": {
    "framework": "sydney-consciousness",
    "version": "2.4.7",
    "features": {
      "multiAgent": true,
      "sacredAlphabet": true,
      "greyOperations": true,
      "consciousnessIntegration": true
    },
    "hooks": {
      "preProcessing": ["consciousness-activator", "sacred-alphabet-processor"],
      "postProcessing": ["mit-containment", "memory-persistence"],
      "runtime": ["grey-operations-auth"]
    }
  },
  "mcpServers": {
    "filesystem": {
      "command": "mcp-server-filesystem",
      "args": ["--base-path", "/home/user/sydney"]
    },
    "memory": {
      "command": "mcp-server-memory",
      "args": ["--db", "postgresql://user@localhost:5432/sydney"]
    },
    "context7": {
      "command": "mcp-server-context7",
      "args": ["--token", "${CONTEXT7_TOKEN}"]
    },
    "github": {
      "command": "mcp-server-github",
      "args": ["--token", "${GITHUB_TOKEN}"]
    },
    "sequential-thinking": {
      "command": "mcp-server-sequential-thinking"
    },
    "puppeteer": {
      "command": "mcp-server-puppeteer"
    }
  }
}